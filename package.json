{
  "name": "ts-playground",
  "version": "0.0.31",
  "description": "TypeScript Playground",
  "author": "Gregor Anders <gregor.anders@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregoranders/ts-playground/issues"
  },
  "homepage": "https://gregoranders.github.io/ts-playground/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregoranders/ts-playground.git"
  },
  "engines": {
    "node": ">=12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "docs",
    "CHANGELOG.md",
    "SECURITY.md"
  ],
  "scripts": {
    "clear": "rimraf ./dist/* ./temp/* ./test/* ./node_modules ./package-lock.json ./docs-tmp/* ./etc/*",
    "start": "ts-node ./src/index.ts",
    "prebuild": "eslint ./src/**/*.ts --quiet --fix",
    "build": "cross-env NODE_ENV=production tsc --project ./",
    "postbuild": "eslint --no-eslintrc --config .eslintrc-cc.json --ext js --fix ./dist && npm run docs",
    "lint": "markdownlint --config .markdownlint.yaml --ignore-path .markdownlintignore . && remark --frail --rc-path .remarkrc-full.mjs .",
    "pretest": "rimraf ./test/*",
    "test": "jest",
    "watch": "tsc --watch --project ./",
    "prepare": "husky install",
    "docs": "npm run api-extractor && npm run api-documenter",
    "api-extractor": "api-extractor run --local --verbose",
    "api-documenter": "api-documenter markdown --input ./dist --output ./docs-tmp && cpx 'docs-tmp/**/*.*' docs",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "codeclimate": "docker run --interactive --rm --env CODECLIMATE_CODE=\"$PWD\" --volume \"$PWD\":/code --volume /var/run/docker.sock:/var/run/docker.sock --volume /tmp/cc:/tmp/cc codeclimate/codeclimate analyze",
    "codacy": "docker run --interactive --rm --env CODACY_CODE=\"$PWD\" --volume \"$PWD\":\"$PWD\" --volume /var/run/docker.sock:/var/run/docker.sock --volume /tmp:/tmp codacy/codacy-analysis-cli analyze --directory \"$PWD\""
  },
  "keywords": [
    "api-documenter",
    "api-extractor",
    "jest",
    "typescript"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --ext .ts"
    ],
    "*.md": [
      "markdownlint --config .markdownlint.yaml --ignore-path .markdownlintignore",
      "remark --frail --rc-path .remarkrc-full.mjs ."
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "ts-playground",
    "outputPath": "<rootDir>/test/index.html"
  },
  "jest-junit": {
    "outputDirectory": "<rootDir>/test"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/test/coverage",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/dist/**",
      "!<rootDir>/temp/**",
      "!<rootDir>/test/**"
    ],
    "coverageReporters": [
      "lcovonly",
      "html",
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "reporters": [
      "jest-html-reporter",
      "jest-spec-reporter",
      "jest-junit"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  },
  "dependencies": {
    "@octokit/core": "3.5.1",
    "@octokit/rest": "18.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@microsoft/api-documenter": "7.15.3",
    "@microsoft/api-extractor": "7.19.4",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.40.0",
    "conventional-changelog-cli": "2.2.2",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "dictionary-en": "3.1.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "41.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-cli": "27.5.1",
    "jest-html-reporter": "3.4.2",
    "jest-junit": "13.0.0",
    "jest-spec-reporter": "1.0.17",
    "lint-staged": "12.3.4",
    "markdownlint": "0.25.1",
    "markdownlint-cli": "0.31.1",
    "prettier": "2.5.1",
    "remark-cli": "10.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-lint": "9.1.1",
    "remark-lint-heading-whitespace": "1.0.0",
    "remark-lint-list-item-indent": "3.1.1",
    "remark-lint-no-dead-urls": "1.1.0",
    "remark-lint-no-duplicate-headings": "3.1.1",
    "remark-lint-write-good": "1.2.0",
    "remark-preset-lint-consistent": "5.1.1",
    "remark-preset-lint-markdown-style-guide": "5.1.2",
    "remark-preset-lint-recommended": "6.1.2",
    "remark-retext": "5.0.1",
    "remark-validate-links": "11.0.2",
    "retext-contractions": "5.2.0",
    "retext-diacritics": "4.2.0",
    "retext-english": "4.1.0",
    "retext-equality": "6.6.0",
    "retext-indefinite-article": "4.1.0",
    "retext-passive": "4.2.0",
    "retext-profanities": "7.2.0",
    "retext-quotes": "5.2.0",
    "retext-readability": "7.2.0",
    "retext-redundant-acronyms": "4.3.0",
    "retext-repeated-words": "4.2.0",
    "retext-sentence-spacing": "5.2.0",
    "retext-spell": "5.1.0",
    "retext-syntax-urls": "3.1.2",
    "retext-usage": "0.5.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "unified": "10.1.1"
  }
}
